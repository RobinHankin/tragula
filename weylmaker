#! /bin/bash

base_dir=~/rstudio

cp ${base_dir}/weyl/inst/weyl_tragula.Rnw ./example_article_weyl.Rnw
cp ${base_dir}/weyl/inst/weyl.bib .
cp ${base_dir}/weyl/man/figures/weyl.png .
R CMD Sweave example_article_weyl.Rnw
pdflatex example_article_weyl
bibtex example_article_weyl
pdflatex example_article_weyl
pdflatex example_article_weyl
